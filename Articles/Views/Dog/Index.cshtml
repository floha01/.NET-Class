@model List<Dog>

<button type="button" class="btn btn-primary">Add dog</button>

<div class="table-responsive">
    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Race</th>
                <th scope="col">Age</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var entry in Model)
            {
                <tr>
                    <td>@entry.Id</td>
                    <td>@entry.Name</td>
                    <td>@entry.Race</td>
                    <td>@entry.Age</td>
                </tr>
            }

    </table>
</div>